{
  "extends": ["alloy", "alloy/typescript", "alloy/react"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-return-assign": "off",
    "no-useless-concat": "off",
    "react/no-unescaped-entities": "off",
    "@typescript-eslint/no-unused-vars": ["error", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }]
  }
}
